@charset "utf-8";

.pinned-comment-bubble-arrow { height: 0px; width: 0px; border-style: solid; content: ""; }

.arrow-outer { position: relative; left: 50px; border-color: rgb(238, 238, 238) rgba(238, 238, 238, 0); border-width: 0px 21px 20px 0px; }

.arrow-inner { position: absolute; top: 4px; left: 2px; border-color: rgb(255, 255, 255) transparent; border-width: 0px 21px 20px 0px; }

.a-size-mini.c7yBadgeAUI { font-size: 9px; }

.c7yTopDownDashedStrike { border-top: 1px dashed rgb(169, 169, 169); border-bottom: 1px dashed rgb(169, 169, 169); }

.c7yBadgeAUI { text-transform: uppercase; letter-spacing: 0.5px; padding: 2px; white-space: nowrap; }

.reviews-content { min-height: 100px; overflow-wrap: break-word; }

.reviews-display-ad { overflow-y: hidden; max-height: 350px; }

.enthusiast-badge, .enthusiast-badge:link, .enthusiast-badge:visited { color: rgb(83, 128, 0); text-decoration: none; }

.review { max-width: 80em; }

.genome-widget-row { display: table; }

.profile-widget-with-avatar { display: table-cell; }

.badges-genome-widget { display: table-cell; vertical-align: middle; }

.cr-skyfall-feedback-section { margin: 0rem -0.8rem; padding: 0.6rem 0.8rem; max-height: 57px; }

.cr-skyfall-whats-this-outer-wrapper { position: relative; }

.cr-skyfall-whats-this-inner-wrapper { display: inline-block; position: absolute; top: -12px; left: -3px; width: 40px; height: 40px; cursor: pointer; }

.cr-skyfall-whats-this { background-size: 18px 18px; }

.cr-skyfall-whats-this-base { background-image: url("https://m.media-amazon.com/images/G/01/AUIClients/CustomerReviewsMedleyAssets-help1x-cf28974086b45c312df703786cff049ad457f859._V2_.png"); background-repeat: no-repeat; width: 18px; height: 18px; display: inline-block; margin: 11px; }

@media only screen and (min-resolution: 1.25dppx) {
  .cr-skyfall-whats-this-base { background-image: url("https://m.media-amazon.com/images/G/01/AUIClients/CustomerReviewsMedleyAssets-help2x-ca432508ba427029601f4a62c02a5f4565b6b36a._V2_.png"); background-repeat: no-repeat; width: 18px; height: 18px; display: inline-block; margin: 11px; }
}

@media only screen and (min-resolution: 3dppx) {
  .cr-skyfall-whats-this-base { background-image: url("https://m.media-amazon.com/images/G/01/AUIClients/CustomerReviewsMedleyAssets-help3x-0efaee705108188801791c9a34bf90ed0904b7fa._V2_.png"); background-repeat: no-repeat; width: 18px; height: 18px; display: inline-block; margin: 11px; }
}

.writeReviewButton.cm-cr-button-wide { width: 300px; }

.cm-cr-dp-filter-dropdown { width: 135px; }

.cm_cr_grid_center_left { width: 9.5%; max-width: 120px; }

.cm_cr_grid_center_right { max-width: 680px; width: 90.5%; }

.reviews-loading { width: 100%; height: 100%; position: absolute; background-repeat: no-repeat; background-position: center top; background-color: rgb(255, 255, 255); z-index: 99; opacity: 0.5; background-image: url("https://images-na.ssl-images-amazon.com/images/G/01/amazonui/loading/spinner_4x._V1_.gif"); }

.cr-vote-button { width: 35px; }

.cr-vote-button-margin { margin-top: -1px; display: inline-block; }

.cr-helpful-button { margin-right: 5px; vertical-align: middle; }

.cr-helpful-text { width: 77px; }

.cr-footer-line-height { line-height: 30px; }

.reviews-customer-images .review-image-tile { height: auto; display: inline; }

.review-image-tile { height: 88px; background-position: center center; background-repeat: no-repeat; background-size: cover; overflow: hidden; display: inline-block; margin-right: 1px; margin-bottom: 4px; width: auto; cursor: pointer; }

.review-image-tile-section { overflow: hidden; }

.reviewsLightbox { width: 100%; height: 100%; font-family: arial; }

.cm-cr-image-tile { margin-left: 4px; }

.review .video-block { float: left; height: 250px; width: 360px; margin-right: 16px; }

.reviews-highlight { background-color: rgb(255, 235, 183); height: 40px; }

.cr-read-more .reviews-highlight { background-color: inherit; height: inherit; }
